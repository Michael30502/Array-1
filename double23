public boolean double23(int[] nums) {
  
  boolean bool= false;

  if(nums.length==2){
  if(nums[0] ==2&&nums[1]==2)
    bool = true;
  if(nums[0] ==3&&nums[1]==3)
  bool = true;
}
  if(nums.length == 3){
  if((nums[0] ==2&&nums[1]==2||nums[0] ==2&&nums[2]==2||nums[1] ==2&&nums[2]==2)||(nums[0] ==3&&nums[1]==2||nums[0] ==3&&nums[2]==3||nums[1] ==3&&nums[2]==3));
  bool =true;
  

  }
return bool;
  
}
